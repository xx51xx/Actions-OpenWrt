
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'west'
	option use_ipv6 '0'
	option enabled '0'
	option lookup_host '-'
	option update_url 'https://api.west.cn/API/v2/domain/dns/?act=dnsrec.update&domain=[USERNAME]&hostname=[DOMAIN]&record_value=[IP]&apidomainkey=[PASSWORD]'
	option domain '-'
	option username '-'
	option password '-'
	option ip_source 'web'
	option ip_url 'https://4.ipw.cn'
	option use_syslog '0'
	option use_logfile '0'
	option check_interval '60'
	option check_unit 'minutes'
	option force_interval '72'
	option force_unit 'hours'
	option retry_interval '3'
	option retry_unit 'seconds'

config service 'west_ipv6'
	option use_ipv6 '1'
	option enabled '0'
	option lookup_host '-'
	option update_url 'https://api.west.cn/API/v2/domain/dns/?act=dnsrec.update&domain=[USERNAME]&hostname=[DOMAIN]&record_value=[IP]&apidomainkey=[PASSWORD]'
	option domain '-'
	option username '-'
	option password '-'
	option ip_source 'web'
	option ip_url 'https://6.ipw.cn'
	option use_syslog '0'
	option use_logfile '0'
	option check_interval '60'
	option check_unit 'minutes'
	option force_interval '72'
	option force_unit 'days'
	option retry_interval '3'
	option retry_unit 'seconds'

