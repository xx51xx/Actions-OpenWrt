config wifi-device 'radio0'
        option type 'mac80211'
        option path 'platform/10180000.wmac'
        option band '2g'
        option channel 'auto'
        option country 'CN'
        option legacy_rates '1'
        option mu_beamformer '0'

config wifi-iface 'default_radio0'
        option device 'radio
        option network 'lan'
	    option mode 'ap'
	    option ssid '+_+!'
	    option encryption 'sae-mixed'
	    option key '00665512'
